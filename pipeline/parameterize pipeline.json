{
	"name": "parameterize pipeline",
	"properties": {
		"activities": [
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('metadata')",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy data1",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "DelimitedTextSource",
									"storeSettings": {
										"type": "AzureBlobFSReadSettings",
										"recursive": true,
										"enablePartitionDiscovery": false
									},
									"formatSettings": {
										"type": "DelimitedTextReadSettings"
									}
								},
								"sink": {
									"type": "AzureSqlSink",
									"writeBehavior": "insert",
									"sqlWriterUseTableLock": false,
									"tableOption": "autoCreate",
									"disableMetricsCollection": false
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "datalakedatasetparam",
									"type": "DatasetReference",
									"parameters": {
										"container": {
											"value": "@item().Inputcontainer",
											"type": "Expression"
										},
										"directory": {
											"value": "@item().Inputdirectory",
											"type": "Expression"
										},
										"filename": {
											"value": "@item().Inputfile",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "sqldatasetparamterized",
									"type": "DatasetReference",
									"parameters": {
										"sqlschema": {
											"value": "@item().schema",
											"type": "Expression"
										},
										"sqltablename": {
											"value": "@item().Outputtable",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"container": {
				"type": "string",
				"defaultValue": "input"
			},
			"directory": {
				"type": "string"
			},
			"filename": {
				"type": "string",
				"defaultValue": "Department1.csv"
			},
			"sqlschema": {
				"type": "string",
				"defaultValue": "dbo"
			},
			"sqltablename": {
				"type": "string",
				"defaultValue": "department"
			}
		},
		"variables": {
			"metadata": {
				"type": "Array",
				"defaultValue": [
					"[{\"Inputfile”:”Department1.csv”",
					"”Inputcontainer\":\"input\"",
					"\"Inputdirectory”:”folder”",
					"”Outputtable”:”x”",
					"”schema”:”dbo”}",
					" [{\"Inputfile”:”Department2.csv”",
					"”Inputcontainer\":\"input\"",
					"\"Inputdirectory”:”folder”",
					"”Outputtable”:”y”",
					"”schema”:”dbo”}",
					" [{\"Inputfile”:”Sample_data.csv”,”Inputcontainer\":\"input\",\"Inputdirectory”:”folder”,”Outputtable”:”z”,”schema”:”dbo”}]"
				]
			}
		},
		"annotations": []
	}
}