									"directory": {
										"value": "@pipeline().parameters.directory",
										"type": "Expression"
									},
									"filename": {
										"value": "@pipeline().parameters.filename",
										"type": "Expression"
									}
								}
							}
						],
						"outputs": [
							{
								"referenceName": "sqldatasetparamterized",
								"type": "DatasetReference",
								"parameters": {
									"sqlschema": {
										"value": "@pipeline().parameters.sqlschema",
										"type": "Expression"
									},
									"sqltablename": {
										"value": "@pipeline().parameters.sqltablename",
										"type": "Expression"
