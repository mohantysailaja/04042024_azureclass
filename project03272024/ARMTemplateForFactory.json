					"directory": {
						"type": "string"
					},
					"filename": {
						"type": "string",
						"defaultValue": "Department1.csv"
					},
					"sqlschema": {
						"type": "string",
						"defaultValue": "dbo"
					},
					"sqltablename": {
						"type": "string",
						"defaultValue": "department"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/datalakedatasetparam')]",
				"[concat(variables('factoryId'), '/datasets/sqldatasetparamterized')]"
			]
		},
		{
